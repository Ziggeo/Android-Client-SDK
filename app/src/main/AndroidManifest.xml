<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ziggeo.demo">

    <application
        android:allowBackup="true"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat">

        <activity android:name="com.ziggeo.androidsdk.demo.FullscreenRecorderActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name="com.ziggeo.androidsdk.demo.EmbeddedRecorderActivity" />

        <activity android:name="com.ziggeo.androidsdk.demo.CameraViewActivity" />

    </application>

</manifest>
